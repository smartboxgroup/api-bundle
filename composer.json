{
    "name": "smartbox/api-bundle",
    "description": "Smartbox Api Bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Smartbox\\ApiBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Smartbox\\ApiRestClient\\": "Command/SDK/"
        }
    },
    "require": {
        "php": ">=5.6",
        "symfony/symfony": ">=2.7.0",
        "doctrine/annotations": "1.2.*",
        "symfony/assetic-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": ">=4.0",
        "sensio/framework-extra-bundle": "^3.0",
        "friendsofsymfony/rest-bundle": "^1.7",
        "jms/serializer-bundle": "^0.13.0",
        "nelmio/api-doc-bundle": "^2.11.2",
        "noxlogic/ratelimit-bundle": "^1.5",
        "snc/redis-bundle": "^1.1",
        "predis/predis": "^1.0",
        "smartbox/core-bundle": "~1.0.0",
        "smartbox/besimple-soap": "~1.0.0",
        "nikic/php-parser": "^2.0@dev"
    },
    "require-dev": {
        "phpunit/php-code-coverage": "~4.0",
        "phpunit/phpunit-mock-objects": "~3.2",
        "phpunit/phpunit": "~5.4",
        "sensio/generator-bundle": "~2.3",
        "digitalnature/php-ref": "dev-master",
        "guzzlehttp/guzzle": "~3.8"
    },
    "config": {
         "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "prefer-stable" : true
}
