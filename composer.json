{
    "name": "smartbox/api-bundle",
    "description": "Smartbox Api Bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Smartbox\\ApiBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Smartbox\\ApiRestClient\\": "ApiRestClient"
        }
    },
    "require": {
        "php": ">=7.2",
        "symfony/symfony": "^2.8|^3.0|^4.0",
        "symfony/assetic-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": ">=4.0",
        "sensio/framework-extra-bundle": "^3.0",
        "friendsofsymfony/rest-bundle": "^1.7",
        "nelmio/api-doc-bundle": "^2.11.2",
        "noxlogic/ratelimit-bundle": "^1.5",
        "snc/redis-bundle": "^1.1",
        "predis/predis": "^1.0",
        "nikic/php-parser": "^2.0@dev",
        "psr/cache": "^1.0",
        "smartbox/core-bundle": "dev-feature/update_jms_serializer",
        "jms/serializer-bundle": "*",
        "smartbox/besimple-soap": "^1.0"
    },
    "require-dev": {
        "digitalnature/php-ref": "dev-master",
        "guzzlehttp/guzzle": "^6.3",
        "sensio/generator-bundle": "~2.3",
        "symfony/phpunit-bridge": "*",
        "phpspec/prophecy": "^1.7",
        "symfony/cache": "~3.0 || ~4.0"
    },
    "config": {
         "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "prefer-stable" : true
}
